//Shuffle deck

function shuffleDeck() {
    console.log('Cards to shuffle', allCards);
    const shuffledCards = shuffle(allCards);
    for (let index = deck.length; index >= 0; index--) {
        deck.appendChild(shuffledCards);
    }
}

    shuffleDeck();


